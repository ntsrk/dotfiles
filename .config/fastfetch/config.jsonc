{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",

  "logo": {
    "type": "kitty-direct",
    // "source": "~/Pictures/profile-fav/wallhaven-wezvyp.jpg",
    "source": "$HOME/Pictures/profile-fav/gengar_profile_pic.jpeg",
    // "source": "~/Pictures/profile-fav/wallhaven-l8127l.png",
    // "width": 30,
    // "height": 15,
    "padding": {
        "top": 1,
        "left": 2,
        "right": 4
    }
  },

  "modules": [
    //"separator",
    {
      "type": "custom",
      "format": "\u001b[90m┌───────────────────────────────────────────────┐"
    },
    {
      "type": "title",
      "key": "  Host",
      "format": "{1}@{2}",
      "keyColor": "yellow"
    },
    {
      "type": "host",
      "key": "󰌢  Chassis",
      "format": "{1}",
      "keyColor": "yellow"
    },
    {
      "type": "os",
      "format": "{3}",
      "key": "󰣭  Distro",
      "keyColor": "yellow"
    },
    {
      "type": "kernel",
      "key": "  Kernel",
      "keyColor": "yellow",
      "format": "{2}"
    },
    {
      "type": "shell",
      "key": "  Shell",
      "keyColor": "yellow",
      "format": "{1} {4}"
    },
    {
      "type": "packages",
      "key": "󰏗  Packages",
      "keyColor": "yellow"
    },
    {
      "type": "wm",
      "key": "  WM",
      "keyColor": "yellow",
      "format": "{1} {3}"
    },
    {
      "type": "custom",
      "format": "\u001b[90m└───────────────────────────────────────────────┘"
    },

    "break",

    {
      "type": "custom",
      "format": "\u001b[90m┌───────────────────────────────────────────────┐"
    },
    {
      "type": "cpu",
      "key": "  CPU",
      "keyColor": "yellow",
      "format": "{name}"
    },
    {
      "type": "gpu",
      "key": "󰢮  GPU",
      "keyColor": "yellow",
      "format": "{vendor} {name}"
    },
    {
      "type": "memory",
      "key": "  RAM",
      "keyColor": "yellow",
      "format": "{1} / {2}"
    },
    {
      "type": "swap",
      "key": "󰯍  Swap",
      "keyColor": "yellow",
      "format": "{1} / {2}"
    },
    {
      "type": "disk",
      "keyColor": "yellow",
      "key": "  Disk",
      "format": "[{mountpoint}] - {size-used} / {size-total}"
    },
    {
      "type": "display",
      "keyColor": "yellow",
      "key": "󰍹  Display",
      "format": "{name}, {width}x{height} @ {refresh-rate}Hz, {inch}\" {type}"
    },
    //"localip",
    {
      "type": "custom",
      "format": "\u001b[90m└───────────────────────────────────────────────┘"
    },

    //"battery",
    //"poweradapter",
    //"locale",
    //"colors"
    "break",

    {
      "type": "custom",
      "format": "\u001b[90m┌───────────────────────────────────────────────┐"
    },
    {
      "type": "uptime",
      "key": "  Uptime",
      "keyColor": "yellow"
    },
    {
      "type": "datetime",
      "key": "  Date",
      "keyColor": "yellow",
    "format": "{day-in-month}.{month-pretty}.{year} {hour-pretty}:{minute-pretty}"
    },
    {
      "type": "custom",
      "format": "\u001b[90m└───────────────────────────────────────────────┘"
    },
    // #############################################
    "break",

    {
      "type": "custom",
      "format": "\u001b[90m┌───────────────────────────────────────────────┐"
    },
    //"de",
    //"wmtheme",
    {
      "type": "theme",
      "key": "󰔎 Theme",
      "keyColor": "yellow"
    },
    {
      "type": "icons",
      "key": " Icons",
      "keyColor": "yellow"
    },
    {
      "type": "font",
      "key": " Font",
      "keyColor": "yellow"
    },
    {
      "type": "terminalfont",
      "key": " Terminal Font",
      "keyColor": "yellow"
    },
    {
      "type": "cursor",
      "key": "󰇀 Cursor",
      "keyColor": "yellow"
    },
    //"terminal",
    
    {
      "type": "custom",
      "format": "\u001b[90m└───────────────────────────────────────────────┘"
    }
  ]
}

